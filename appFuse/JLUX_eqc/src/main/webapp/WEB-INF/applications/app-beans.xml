<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:mvc="http://www.springframework.org/schema/mvc"
	xsi:schemaLocation="
 
http://www.springframework.org/schema/beans

http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="appStartedEventHandler" class="com.expedia.lodging.connectivity.init.LocaleMappingLoadService"/>
	<bean id="contentRepo" class="com.expedia.lodging.connectivity.repository.imp.ContentImp"></bean>
	<bean id="EQCLocaleRepo" class="com.expedia.lodging.connectivity.repository.imp.EQCLocaleImp"></bean>
	<bean id="pageRepo" class="com.expedia.lodging.connectivity.repository.imp.PagesImp"></bean>
	
	<!-- O/X mapping -->
    <bean id="convertor" class="com.expedia.lodging.connectivity.DTO.LeftNavMenuXMLConvertor">
        <property name="marshaller" ref="castorMarshaller" />
        <property name="unmarshaller" ref="castorMarshaller" />
    </bean>
    <bean id="castorMarshaller" class="org.springframework.oxm.castor.CastorMarshaller"/>
	<!-- O/X mapping -->
	
	
</beans>